<% include ../partials/header %>

<div class="container">
    <img src="/images/gif1.gif" style="margin-top: 30px;" class="img-fluid" alt="">
</div>

<div class="container" style="margin-top: 30px;">
    <div class="row row-cols-1 row-cols-md-4" data-aos="zoom-in">
      <% greaterThan50.forEach((product) => { %>
        <div class="col-lg-3 col-md-4 col-sm-6 mb-3">
            <div class="card">
                <div class="container" style="text-align: center; margin-top: 20px;">
                    <a href="/products/<%= product.category %>/<%= product.slug %>">
                        <img src="/product_images/<%= product._id %>/<%= product.image %>" class="card-img" alt="...">
                    </a>
                </div>
                <div style="text-align: center;" class="card-body">
                    <h5 class="card-title"><%= product.name %></h5>
                    <div class="star-rate">
                        <span class="fa fa-star checked"></span>
                        <span class="fa fa-star checked"></span>
                        <span class="fa fa-star checked"></span>
                        <span class="fa fa-star"></span>
                        <span class="fa fa-star"></span>
                    </div>
                    <% var salePrice = product.salePrice.toLocaleString("vi", { style: "currency", currency: "VND", })%>
                    <% var price = product.price.toLocaleString("vi", { style: "currency", currency: "VND", })%>
                    <% if (product.sale != 0) { %>
                    <p class="original-price"><strike><%= price %></strike> <strong><i>-<%= product.sale %>%</i></strong></p>
                    <p class="product-price"><%= salePrice %></p>
                    <% } else { %>
                    <br>
                    <p class="product-price"><%= price %></p>
                    <% } %>
                    <a style="background-color: #51BCB1; padding: 10px 30px;"
                        class="btn btn-default button-back color-1" href="/cart/add/<%= product.slug %>"
                        role="button">Thêm vào giỏ</a>
                </div>
            </div>
        </div>
        <% }); %>
    </div>
</div>

